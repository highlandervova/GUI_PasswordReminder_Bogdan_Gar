<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
<left>
    <AnchorPane prefHeight="400.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <children>
            <ImageView fitHeight="400.0" fitWidth="200.0" pickOnBounds="true">
                <image>
                    <Image url="@../image/firstimage.png" />
                </image></ImageView>
        </children>
    </AnchorPane>
</left>
<center>
    <AnchorPane prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #EBF4FE;" BorderPane.alignment="CENTER">
        <children>
            <Label layoutX="14.0" layoutY="45.0" prefHeight="30.0" prefWidth="80.0" text="Адрес" textOverrun="CLIP">
                <font>
                    <Font name="Century Gothic" size="18.0" />
                </font>
            </Label>
            <TextField fx:id="addressField" layoutX="94.0" layoutY="47.0" prefHeight="26.0" prefWidth="200.0" promptText="Введите адрес ресурса">
                <font>
                    <Font size="10.0" />
                </font>
            </TextField>
            <Label layoutX="14.0" layoutY="90.0" prefHeight="30.0" prefWidth="80.0" text="Логин" textOverrun="CLIP">
                <font>
                    <Font name="Century Gothic" size="18.0" />
                </font>
            </Label>
            <Label layoutX="14.0" layoutY="140.0" prefHeight="30.0" prefWidth="80.0" text="Пароль" textOverrun="CLIP">
                <font>
                    <Font name="Century Gothic" size="18.0" />
                </font>
            </Label>
            <PasswordField fx:id="passwordField1" layoutX="94.0" layoutY="142.0" prefHeight="26.0" prefWidth="200.0" promptText="Введите пароль">
                <font>
                    <Font size="10.0" />
                </font>
            </PasswordField>
            <TextField fx:id="loginField" layoutX="94.0" layoutY="92.0" prefHeight="26.0" prefWidth="200.0" promptText="Введите логин">
                <font>
                    <Font size="10.0" />
                </font>
            </TextField>
            <Label layoutX="144.0" layoutY="175.0" prefHeight="34.0" prefWidth="80.0" text="       ИЛИ">
                <font>
                    <Font name="Century Gothic" size="14.0" />
                </font>
            </Label>
            <Label layoutX="15.0" layoutY="200.0" prefHeight="30.0" prefWidth="370.0" text="создайте генерируемый пароль введя количество символов:">
                <font>
                    <Font name="Century Gothic" size="12.0" />
                </font>
            </Label>
            <TextField fx:id="passwordField2" layoutX="162.0" layoutY="230.0" onKeyTyped="#passwordField2KeyTyped" prefHeight="26.0" prefWidth="52.0" />
            <Button fx:id="exitButton" layoutX="14.0" layoutY="348.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #99C1EA;" text="Выход">
                <font>
                    <Font name="Century Gothic" size="10.0" />
                </font>
            </Button>
            <Button fx:id="listButton" layoutX="230.0" layoutY="348.0" mnemonicParsing="false" onAction="#listButtonOnAction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #99C1EA;" text="Список">
                <font>
                    <Font name="Century Gothic" size="10.0" />
                </font>
            </Button>
            <Button fx:id="saveButton" layoutX="314.0" layoutY="348.0" mnemonicParsing="false" onAction="#saveButtonOnAction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #99C1EA;" text="Сохранить">
                <font>
                    <Font name="Century Gothic" size="10.0" />
                </font>
            </Button>
            <Label fx:id="messageLabel" alignment="CENTER" layoutX="113.0" layoutY="268.0" prefHeight="18.0" prefWidth="142.0">
                <font>
                    <Font name="Century Gothic" size="14.0" />
                </font>
            </Label>
            <Label fx:id="messageLabel2" layoutX="112.0" layoutY="303.0" prefHeight="26.0" prefWidth="225.0">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
            </Label>
        </children></AnchorPane>
</center>
</BorderPane>
